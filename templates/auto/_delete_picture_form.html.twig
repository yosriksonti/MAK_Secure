<form method="post" action="{{ path('auto_delete_picture', {'id': picture.id}) }}"onsubmit="return confirm('Are you sure you want to delete this item?');">
    <button class="btn" ><i class="fa-solid fa-trash"></i></button>
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ picture.id) }}">
</form>
